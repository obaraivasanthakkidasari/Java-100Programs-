public class PerfectSquareNumber {
	static boolean isPerfectSquare(int x) {
		if(x>= 0) {
			int sr = (int)Math.sqrt(x);
			return ((sr*sr)== x);
					}
		return false;
	}

	public static void main(String[] args) {
		
		int x= 81;
		
		if(isPerfectSquare(x))
			System.out.println("True");
		else
			System.out.println("Flase");
	}
