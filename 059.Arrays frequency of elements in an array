public class FrequencySimple {
    public static void main(String[] args) {
        int[] arr = {5, 2, 8, 1, 3, 2, 5, 1, 5};

        System.out.println("Frequency of elements:");

        for (int i = 0; i < arr.length; i++) {
            boolean counted = false;
            for (int j = 0; j < i; j++) if (arr[i] == arr[j]) counted = true;
            if (!counted) {
                int count = 0;
                for (int k = 0; k < arr.length; k++) if (arr[i] == arr[k]) count++;
                System.out.println(arr[i] + " : " + count);
            }
        }
    }
}
