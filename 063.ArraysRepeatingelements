
import java.util.HashSet;
public class ArraysRepeatingelements {
    public static void main(String[] args) {
        int[] arr = {5, 2, 8, 1, 3, 2, 5, 1, 5};

        HashSet<Integer> seen = new HashSet<>();
        HashSet<Integer> repeating = new HashSet<>();

        for (int num : arr) {
            if (seen.contains(num)) {
                repeating.add(num);
            } else {
                seen.add(num);
            }
        }

        System.out.println("Repeating elements in array: " + repeating);
    }
}
