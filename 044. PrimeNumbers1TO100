public class PrimeNumbers1TO100 {
    public static void main(String[] args) {
        for (int num = 2; num <= 100; num++) {
            int count = 0;
            for (int i = 1; i <= num; i++) {
                if (num % i == 0) {
                    count++;
                }
            }
            if (count == 2) {  // Prime numbers have only 2 factors: 1 and itself
                System.out.println(num);
            }
        }
}
}
