
public class AreaOfCircle {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter Radius: ");
        float r = sc.nextFloat();
        double area = 3.14 * r * r;
        System.out.println("Area of Circle = " + area);
        sc.close();
    }
}
